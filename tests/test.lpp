class animal

    name {get}

    function new(name)
        self.name = name
    end

    function getname()
        return self.name
    end

    function setname(name)
        self.name = name
        if(#self.name > 100)then 
            print("that's a long name!")
        end
    end

end

local function b() end

local b = new animal()
b:setname("Test")
print(b:getname())